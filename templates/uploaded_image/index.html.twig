{% extends 'base.html.twig' %}

{% block title %}UploadedImage index{% endblock %}

{% block body %}
    <h1>UploadedImage index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Image_url</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for uploaded_image in uploaded_images %}
            <tr>
                <td>{{ uploaded_image.id }}</td>
                <td>{{ uploaded_image.imageUrl }}</td>
                <td>
                    <a href="{{ path('app_uploaded_image_show', {'id': uploaded_image.id}) }}">show</a>
                    <a href="{{ path('app_uploaded_image_edit', {'id': uploaded_image.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_uploaded_image_new') }}">Create new</a>
{% endblock %}
